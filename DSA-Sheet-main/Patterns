void pat11(int n)
{
    for (int i = 1; i <= n; i++)
    {
        int nm;
        for (int j = 1; j <= i; j++)
        {
            if (j % 2 == 0)
            {
                nm = 0;
            }
            else
            {
                nm = 1;
            }
            cout << nm << " ";
        }
        cout << endl;
    }
}




