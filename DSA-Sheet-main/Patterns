void pat20(int n)
{
    int initSpace = 2 * n - 2;
    for (int i = 1; i <= 2 * n - 1; i++)
    {
        //    stars
        int stars = i;
        if (i > n)
            stars = 2 * n - i;
        for (int j = 1; j <= stars; j++)
        {
            cout << "*";
        }

        // space
        for (int j = 1; j <= initSpace; j++)
        {
            cout << " ";
        }

        // stars
        for (int j = 1; j <= stars; j++)
        {
            cout << "*";
        }
        cout << endl;
        if (i < n)
            initSpace -= 2;
        else
            initSpace += 2;
    }
}




