void pat19(int n)
{
    // upper part
    int sp = 0;
    for (int i = 0; i < n; i++)
    {
        // star
        for (int j = 0; j < n - i; j++)
        {
            cout << "*";
        }
        // space
        for (int j = 0; j < sp; j++)
        {
            cout << " ";
        }
        // star
        for (int j = 0; j < n - i; j++)
        {
            cout << "*";
        }
        cout << endl;
        sp += 2;
    }
    // Lower part
    int space = 2 * (n - 1);
    for (int i = 0; i < n; i++)
    {
        // Left stars
        for (int j = 0; j <= i; j++)
        {
            cout << "*";
        }
        // Spaces
        for (int j = 0; j < space; j++)
        {
            cout << " ";
        }
        // Right stars
        for (int j = 0; j <= i; j++)
        {
            cout << "*";
        }
        space -= 2;
        cout << endl;
    }
}




